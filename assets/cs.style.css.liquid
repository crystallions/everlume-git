}

.wls i.fa {
  margin-right: 5px;
}
/*Cart*/
.num-items-in-cart span {
  color: {{settings.header_txt_colour}};
  text-transform: uppercase;
  font-family: {{settings.general_font}};
  font-weight: 400;
  line-height: 20px;
  vertical-align: 2px;
}
.num-items-in-cart span.icon span.number {
  font-size: 13px;
