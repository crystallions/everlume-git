}

/*-------------
header
---------------*/
.templateIndex header#top {
  background: {{settings.header_bg}};
  /*position: fixed;*/
  top: 0;
  left: 0;
  right: 0;
  width: 100%;
  z-index: 999;
}
header#top {
  background: {{settings.bkg-color}};
}

header#top .line {
  border-top: 1px solid rgba(0,0,0,0.1);
  width: 100%;
  height: 1px;
}
.top {
  color: {{settings.header_link_colour}};
  text-transform: uppercase;
  font-weight: 500;
}
.top .text-right {
  width: 100%;
  float: right;
  margin: 0;
  position: relative;
}
.top .phone-shopping span {
  vertical-align: -7px;
  font-size: 11px;
}
.top-navigation {
  font-family: {{settings.general_font}};
  color: {{settings.nav_link_colour}};
  font-size: 14px;
  text-transform: uppercase;
  width: 100%;
  position: relative;
  padding: 0;
}

@media (min-width: 1200px) {
  .top-navigation {
    padding: 0 2.5% 0 8%;
  }
}

.top-navigation > ul {
  display: table;
  table-layout: fixed;
  width: 100%;
}
.top-navigation ul li.top-logo, 
.top-navigation ul li.navigation,
.top-navigation ul li.top-search,
.top-navigation ul li.umbrella {
  display: table-cell;
  vertical-align: middle;
  float: none;
}
.top-navigation ul li.top-logo {
  width: 20%;
  padding: 0;
  left: 3.7%;
  position: relative;
}
.top-navigation ul li.navigation {
  width: 63%;
  padding: 0;
}
.top-navigation ul li.navigation .navbar .navbar-collapse.collapse {
  padding-right: 20px;
}
.top-navigation ul li.top-search .header-search {
  display: table;
  table-layout: fixed;
  margin-left: 9px;
}
.top-navigation ul li.top-search .header-search i.fa {
  vertical-align: 3px;
  color: {{settings.header_txt_colour}};
  font-size: 13px;
  font-weight: 300;
}
#umbrella .cart-link:after {
  content: "";
  background: #b1b1b1;
  width: 1px;
  height: 13px;
  position: absolute;
  top: 0;
  margin: auto;
  left: 0;
  margin-top: 21px;
  display:none;
}
.top-navigation ul li.umbrella {
  text-align: right;
  position: relative;
  padding-right: 0;
  padding-left: 15px;
}
.top-navigation .navbar ul.navbar-nav {
  float: right;
}
input::-webkit-input-placeholder { /* Chrome 21 */
    color: #b4b4b4;
}
input:-moz-placeholder { /* Firefox 17 */
    color: #b4b4b4;
}
input:-ms-input-placeholder { /* IE 10 */
    color: #b4b4b4;
}
#top .is-mobile li .currency i, 
.btn-group, 
.btn-navbar {
  cursor: pointer;
}
.btn-group.open .dropdown-toggle {
  box-shadow: none;
}
#top .top a:hover {
  color: {{settings.b_links_colour_hover}};
}
/*Currencies*/
.dropdown-menu > .active > a, 
.dropdown-menu > .active > a:hover, 
.dropdown-menu > .active > a:focus, 
.dropdown-menu > li > a:hover, 
.dropdown-menu > li > a:focus {
  background: 0;
}
.dropdown-menu {
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  border: 0;
  -webkit-box-shadow: 0 6px 12px rgba(0, 0, 0, 0.175);
  box-shadow: 0 0px 2px rgba(0, 0, 0, 0.175);
}
/*input*/
input:focus,
.dropdown-toggle:focus {
  outline: none;
  box-shadow: none;
}
/*Customer link*/
.customer-links {
  padding-right: 0;
}
.customer-links .my-account {
  padding-right: 20px;
}
.customer-links .login {
