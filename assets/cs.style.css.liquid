/*----------------------------------
** GENERAL
-------------------------------------*/

html, body{
  overflow-x: hidden;
}

a:focus,
a:active{
  background-color: transparent;
}
form{
  margin: 0;
  position: relative;
}
.alert ul{
  list-style: none;
  font-weight: normal;
